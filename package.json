{
  "name": "css-frameworks-ca",
  "version": "1.0.0",
  "description": "Test",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "npm run build && live-server --port=5501",
    "lint": "npm run format && eslint .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test-e2e": "concurrently --kill-others \"npm start\" \"wait-on http://127.0.0.1:5501 && cypress open\""
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@jest/globals": "^29.7.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.0.1",
    "cypress": "^13.15.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^4.0.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "live-server": "^1.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.54.8",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3",
    "node": "^22.10.0",
    "uuid": "^10.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
